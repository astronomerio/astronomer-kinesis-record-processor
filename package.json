{
  "name": "@astronomerio/astronomer-kinesis-record-processor",
  "version": "2.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib -d dist",
    "dev": "kcl-bootstrap -e -p properties/dev.properties -j /usr/bin/java",
    "prepublish": "npm run build && npm run test",
    "start": "kcl-bootstrap -e -p properties/prod.properties -j /usr/bin/java",
    "tail": "tail -f app.log | bunyan -o short",
    "test": "mocha -r babel-register -r babel-polyfill -r dotenv/config test/index.test.js"
  },
  "dependencies": {
    "@astronomerio/astronomer-logger": "^1.1.1",
    "@astronomerio/emitter-s3": "github:astronomerio/emitter-s3#1.0.0",
    "@astronomerio/partitioned-buffer": "github:astronomerio/partitioned-buffer#1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-3": "6.11.0",
    "babel-register": "6.9.0",
    "chai": "^3.5.0",
    "dotenv": "2.0.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.6"
  }
}
